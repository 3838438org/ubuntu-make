<div class="container">
    <div class="leftfloated block30">
    <p class="bigtext">Follow us. Get more information and give us your feedback</p>
    





    <script type="text/javascript" src="/js2/jquery/validate.js"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            var targetFormSubscription = $("#sForm");
            targetFormSubscription.validate({
                submitHandler: function(form) {
                    sendNewsletterSubscription(targetFormSubscription);
                }
            });

        });
        function sendNewsletterSubscription(targetForm) {
            var postData;
            if (targetForm) {
                postData = targetForm.serialize();
                postData = postData.replace("emaiForSubscription", "Email");
            } else {
                postData = "Email=" + $("#emaiForSubscription").val() + "&subId=62&formid=1627";
            }
            $.ajax({
                type: 'POST',
                url: 'https://app-lon02.marketo.com/index.php/leadCapture/save',
                data: postData,
                complete: function() {
                    $("#formContainer").text("You request has been processed!");
                }
            });
        }
    </script>

<dl id="submitionContent" class="subcription subcription_product">
