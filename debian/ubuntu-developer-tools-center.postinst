#!/bin/sh
set -e

case "$1" in
    configure)
        rm -f /etc/bash_completion.d/udtc
        register-python-argcomplete3 umake > /etc/bash_completion.d/umake
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0

